{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "experimentalObjectRestSpread": true,
    "newTarget": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "$": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-underscore-dangle": 0,
    "quotes": 0,
    "eol-last": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-undef": 2,
    "no-multi-spaces": 0,
    "camelcase": 0,
    "new-cap": 0,
    "no-shadow": 0,
    "handle-callback-err": 0,
    "no-path-concat": 0,
    "no-new": 0,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-pascal-case": 1
  }
}
